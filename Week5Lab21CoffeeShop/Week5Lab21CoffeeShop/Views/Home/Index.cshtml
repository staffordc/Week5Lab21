@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>here's coffee</h1>
    <p class="lead">Oh goodness.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    @foreach (var item in ViewBag.Items)
    {
        <div class="col-md-4">
            <h2>@item.ItemName</h2>
            <p>
                <div>@item.LocationFrom</div>
                <div>@item.GrindType</div>
                <div>@item.Shade</div>
                <div>@item.PricePerPound</div>
            </p>
        </div>

    }

 
</div>